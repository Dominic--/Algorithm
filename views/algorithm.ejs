<%- include header %>

<link type="text/css" rel="stylesheet" href="/css/syntax.css"></link>
<script language="javascript" src="/js/syntax.js"></script>
<script language="javascript" src="/js/graph.js"></script>
<script language="javascript">
    $(function() {
        dp.SyntaxHighlighter.ClipboardSwf = '/flash/clipboard.swf';
        dp.SyntaxHighlighter.HighlightAll('code');

        init_array();

        init_affix();
    });
</script>

<div class="content bs-docs-container">
    <div class="row">
        <div class="col-md-9" role="main">
            <div class="bs-docs-section">
                <%- doc.content %>
            </div>
        </div>

        <div class="col-md-3">
            <div class="bs-docs-sidebar hidden-print" role="complementary">
                <ul class="nav bs-docs-sidenav" id="my-affix"></ul>
            </div>
        </div>
    </div>
</div>

<%- include footer %>
